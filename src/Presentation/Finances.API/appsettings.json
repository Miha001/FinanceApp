{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "restrictedToMinimumLevel": "Information",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Key": "",
    "Issuer": "MyAuthServer",
    "Audience": "MyAuthClient",
    "Authority": "Authority",
    "RefreshTokenValidityInDays": 15
  },
  "RedisSettings": {
    "Url": "localhost:6379",
    "InstanceName": "MyApp_"
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowSeconds": 30,
    "QueueLimit": 5
  }
}
