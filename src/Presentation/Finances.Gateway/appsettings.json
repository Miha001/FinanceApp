      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Key": "OjGwxqUcDcwEMsMaueVZRDZUAkgcukaBaatIMTbFXAgaiAJKPeUkJwziutSU",
    "Issuer": "MyAuthServer",
    "Audience": "MyAuthClient",
    "Authority": "Authority",
    "RefreshTokenValidityInDays": 15
  },
  "RedisSettings": {
    "Url": "localhost:6379",
    "InstanceName": "MyApp_"
  }
}
