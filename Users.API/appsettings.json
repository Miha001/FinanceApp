{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Key": "OjGwxqUcDcwEMsMaueVZRDZUAkgcukaBaatIMTbFXAgaiAJKPeUkJwziutSU",
    "Issuer": "MyAuthServer",
    "Audience": "MyAuthClient",
    "Lifetime": "100",
    "Authority": "Authority",
    "RefreshTokenValidityInDays": 15,
    "AccessTokenValidityInDays": 2
  },
  "RedisSettings": {
    "Url": "localhost:6379",
    "InstanceName": "MyApp_"
  }
}
